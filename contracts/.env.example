# AlphaNest Contracts - Environment Configuration
# Copy this file to .env and fill in your values

# ============================================
# REQUIRED: Wallet Private Key
# ============================================
# Your deployer wallet private key (without 0x prefix)
# WARNING: Never commit this file with real keys!
PRIVATE_KEY=your_private_key_here

# ============================================
# RPC URLs
# ============================================
# Get from Alchemy, Infura, or other providers

# Ethereum Mainnet
ETH_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_API_KEY

# Sepolia Testnet
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_API_KEY

# Base Mainnet
BASE_RPC_URL=https://mainnet.base.org

# Base Sepolia Testnet
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# ============================================
# Block Explorer API Keys (for verification)
# ============================================
ETHERSCAN_API_KEY=your_etherscan_api_key
BASESCAN_API_KEY=your_basescan_api_key

# ============================================
# Deployed Contract Addresses (fill after deployment)
# ============================================
# Sepolia Testnet
SEPOLIA_ALPHA_TOKEN=
SEPOLIA_ALPHANEST_CORE=
SEPOLIA_REPUTATION_REGISTRY=
SEPOLIA_CROSS_CHAIN_VERIFIER=
SEPOLIA_TOKEN_FACTORY=
SEPOLIA_ALPHA_GUARD=
SEPOLIA_ALPHA_GUARD_ORACLE=
SEPOLIA_MOCK_USDC=

# Base Mainnet
BASE_ALPHA_TOKEN=
BASE_ALPHANEST_CORE=
BASE_REPUTATION_REGISTRY=
BASE_CROSS_CHAIN_VERIFIER=
BASE_TOKEN_FACTORY=
BASE_ALPHA_GUARD=
BASE_ALPHA_GUARD_ORACLE=
